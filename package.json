  "keywords": [
    "research",
    "papers",
    "bookmarks",
    "academic"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.21.0",
    "lokijs": "^1.5.12",
    "pdf-parse": "^2.4.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/lokijs": "^1.5.14",
    "@types/node": "^22.0.0",
    "@types/pdf-parse": "^1.1.5",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0"
  }
}
