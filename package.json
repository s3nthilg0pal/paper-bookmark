{
  "name": "paper-bookmark",
  "version": "2.0.0",
  "description": "A self-hosted research paper bookmarking system",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit"
