# Paper Bookmark Configuration

# Server
PORT=3000
NODE_ENV=production

# Security
# Generate a random API key: openssl rand -hex 32
# Leave empty to disable API key authentication
API_KEY=

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# CORS (comma-separated origins, or * for all)
CORS_ORIGINS=*

# Database
DB_AUTOSAVE_INTERVAL=5000
